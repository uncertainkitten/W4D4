<h2><%= current_user.email%> was BAND for this post!</h2>

<body>
  <ul><% Band.all.each do |band|%>
    <li><a href="<%= band_url(band)%>"><%= band.name %></a></li>
    <form action="<%= band_url(band)%>" method="post">
      <input type="hidden" name="_method" value="DELETE">
      <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token%>">
      <input type="submit" value="Disband">
    </form>
  <% end%>
  </ul>
</body>
